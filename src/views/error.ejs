<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('./partials/head'); %>
  </head>
  <body class="container">

    <header>
      <%- include('./partials/header'); %>
    </header>

    <main>
      <div class="jumbotron">
        <% if (typeof message !== "undefined") { %>
          <h2>The page could not be displayed for the following reason:</h2>
          <h1><%= message %></h1>
          <h2><%= error.status %></h2>
          <pre><%= error.stack %></pre>
        <% } %>
      </div>
    </main>

    <footer>
      <%- include('./partials/footer'); %>
    </footer>

  </body>
</html>