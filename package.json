{
  "name": "calance-bootstrap-csv",
  "version": "0.0.0",
  "bin": "./src/bin/helpers/www.js",
  "private": true,
  "scripts": {
    "start": "PORT=8000 node ./src/bin/helpers/www",
    "devStart": "PORT=8000 nodemon ./src/bin/helpers/www"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csv-writer": "^1.6.0",
    "debug": "~2.6.9",
    "ejs": "^3.1.9",
    "express": "~4.16.1",
    "express-session": "^1.17.3",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "pkg": "^5.8.0"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "outputPath": "dist",
    "assets": [
      "./node_modules/axios/dist/node/axios.cjs",
      "./snapshot/calance_bootstrap_csv/node_modules/axios/**/*",
      "./src/views/**/*.ejs",
      "./src/public/**/*.js",
      "./src/utils/**/*.js",
      "./src/bin/**/*.js"
    ]
  }
}
